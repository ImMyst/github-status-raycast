{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "github-status",
  "title": "Github Status",
  "description": "Just showing Github status in list",
  "icon": "icon.png",
  "author": "ImMyst",
  "categories": [
    "Developer Tools",
    "Productivity"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "index",
      "title": "Github Status",
      "subtitle": "Quick Github Status",
      "description": "Template for showing a static list and a detail view",
      "mode": "view"
    },
    {
      "name": "menubar",
      "title": "Menubar Github Status",
      "description": "Quick Menu Bar Github Status",
      "mode": "menu-bar",
      "icon": "icon.png",
      "interval": "15m"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.49.0",
    "@raycast/utils": "^1.5.2",
    "dayjs": "^1.11.7"
  },
  "devDependencies": {
    "@raycast/eslint-config": "1.0.5",
    "@types/node": "18.8.3",
    "@types/react": "18.0.9",
    "eslint": "^7.32.0",
    "prettier": "^2.5.1",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "npx @raycast/api@latest publish"
  }
}